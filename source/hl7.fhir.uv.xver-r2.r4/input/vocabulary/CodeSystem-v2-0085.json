  "concept": [
    {
      "code": "A",
      "display": "Amended based on adjustments provided by the Placer (Physician) regarding patient demographics (such as age and/or gender or other patient specific information",
      "designation": [
        {
          "language": "nl",
          "value": "Aangevuld op basis van aanpassingen van de Placer (Arts) met betrekking tot patiënt demografie (zoals leeftijd en/of geslacht of andere patiënt-specifieke informatie"
        }
      ]
    },
    {
      "code": "B",
      "display": "Appended Report - Final results reviewed and further information provided for clarity without change to the original result values.",
      "designation": [
        {
          "language": "nl",
          "value": "Aangevuld verslag - Definitieve resultaten en verdere informatie gegeven voor duidelijkheid zonder dat de oorspronkelijke resultaatwaarden wijzigen"
        }
      ]
    },
    {
      "code": "C",
      "display": "Record coming over is a correction and thus replaces a final result",
      "designation": [
        {
          "language": "de",
          "value": "Korrektur bereits gemeldeter Ergebnisse"
        },
        {
          "language": "nl",
          "value": "Dit record is een correctie en vervangt het definitief resultaat"
        }
      ]
    },
    {
      "code": "D",
      "display": "Deletes the OBX record",
      "designation": [
        {
          "language": "de",
          "value": "Gemeldete Ergebnisse löschen"
        },
        {
          "language": "nl",
          "value": "Verwijdert het OBX record"
        }
      ]
    },
    {
      "code": "F",
      "display": "Final results",
      "designation": [
        {
          "language": "de",
          "value": "Endergebnis"
        },
        {
          "language": "nl",
          "value": "Definitieve resultaten"
        }
      ]
    },
    {
      "code": "I",
      "display": "Specimen in lab; results pending",
      "designation": [
        {
          "language": "de",
          "value": "Probe im Labor - Ergebnisse stehen an"
        },
        {
          "language": "nl",
          "value": "Monster in lab; resultaten volgen"
        }
      ]
    },
    {
      "code": "N",
      "display": "Not asked; used to affirmatively document that the observation identified in the OBX was not sought when the universal service ID in OBR-4 implies that it would be sought.",
      "designation": [
        {
          "language": "de",
          "value": "ungefragte Übertragung"
        },
        {
          "language": "nl",
          "value": "Niet gevraagd; gebruikt om op schrift te stellen dat de observatie geïdentificeerd in de OBX niet werd gevraagd toen de universal section ID in OBR-4 implicileerde dat deze wel werd gezocht."
        }
      ]
    },
    {
      "code": "O",
      "display": "Order detail description only (no result)",
      "designation": [
        {
          "language": "de",
          "value": "Anforderungsbeschreibung (ohne Ergebnisse)"
        },
        {
          "language": "nl",
          "value": "Alleen aanvraag detail (geen resultaat)"
        }
      ]
    },
    {
      "code": "P",
      "display": "Preliminary results",
      "designation": [
        {
          "language": "de",
          "value": "Vorläufige Ergebnisse"
        },
        {
          "language": "nl",
          "value": "Voorlopige resultaten"
        }
      ]
    },
    {
      "code": "R",
      "display": "Results entered -- not verified",
      "designation": [
        {
          "language": "de",
          "value": "Nicht validierte Ergebnisse"
        },
        {
          "language": "nl",
          "value": "Resultaten ingevoerd -- niet geverifieerd"
        }
      ]
    },
    {
      "code": "S",
      "display": "Partial results.   Deprecated. Retained only for backward compatibility as of V2.6.",
      "designation": [
        {
          "language": "de",
          "value": "Teilergebnisse"
        },
        {
          "language": "nl",
          "value": "Gedeeltelijke resultaten. Deprecated. Alleen voor backward compatibiliteit sinds V2.6"
        }
      ]
    },
    {
      "code": "U",
      "display": "Results status change to final without retransmitting results already sent as 'preliminary.'  E.g. radiology changes status from preliminary to final",
      "designation": [
        {
          "language": "de",
          "value": "Vorläufiges Ergebnis wird Endergebnis, da keine Änderung"
        },
        {
          "language": "nl",
          "value": "Result statuswijziging naar Definitief zonder de voorlopige resultaten opnieuw te sturen. Bijv. radiologie wijzigt status van voorlopig naar definitief"
        }
      ]
    },
    {
      "code": "V",
      "display": "Verified - Final results reviewed and confirmed to be correct, no change to result value, normal range or abnormal flag",
      "designation": [
        {
          "language": "nl",
          "value": "Geverifieerd - Definitieve resultaten gereviewd en correct bevonden, geen wijziging aan resultaatwaarde, normaalwaarde of abnormaal indicatie"
        }
      ]
    },
    {
      "code": "W",
      "display": "Post original as wrong, e.g. transmitted for wrong patient",
      "designation": [
        {
          "language": "de",
          "value": "fehlerhafte Übertragung"
        },
        {
          "language": "nl",
          "value": "Oorspronkelijke resultaten verkeerd verstuurd, bijv. voor verkeerde patiënt"

      ],
      "concept": [
        {
          "code": "AIRBORNE",
          "display": "airborne",
          "definition": "Description: Communication of an agent from a living subject or environmental source to a living subject through indirect contact via oral or nasal inhalation.",
          "property": [
            {
              "code": "notSelectable",

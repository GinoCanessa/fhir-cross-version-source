  "description": "FHIR Value set/code system definition for HL7 v2 table 0298 ( CP Range Type)",
  "content": "complete",
  "concept": [
    {
      "code": "F",
      "display": "Flat-rate. Apply the entire price to this interval, do not pro-rate the price if the full interval has not occurred/been consumed",
      "designation": [
        {
          "language": "nl",
          "use": {
            "system": "http://terminology.hl7.org/CodeSystem/designation-usage",
            "code": "display"
          },
          "value": "Vaste prijs. Pas de gehele prijs toe op dit interval, bereken niet naar rato in het geval dat niet het hele interval is opgetreden/is verbruikt"
        }
      ]
    },
    {
      "code": "P",
      "display": "Pro-rate. Apply this price to this interval, pro-rated by whatever portion of the interval has occurred/been consumed",
      "designation": [
        {
          "language": "nl",
          "use": {
            "system": "http://terminology.hl7.org/CodeSystem/designation-usage",
            "code": "display"
          },
          "value": "Pro rato. Pas deze prijs toe op dit interval, naar rato van het deel van het interval dat is opgetreden/is verbruikt"
        }
      ]
    }
  ]
